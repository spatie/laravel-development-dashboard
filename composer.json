{
    "name": "spatie/laravel-development-dashboard",
    "description": "A beautiful dashboard that helps you write quality Laravel apps",
    "keywords": [
        "spatie",
        "laravel-development-dashboard"
    ],
    "homepage": "https://github.com/spatie/laravel-development-dashboard",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian De Deyne",
            "email": "sebastian@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        },
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "spatie/phpunit-snapshot-assertions": "^1.3"
    },
    "require-dev": {
        "orchestra/testbench": "^3.6",
        "phpunit/phpunit": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\DevelopmentDashboard\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\DevelopmentDashboard\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\DevelopmentDashboard\\DevelopmentDashboardServiceProvider"
            ],
            "aliases": {
                "Projectionist": "Spatie\\DevelopmentDashboard\\Facades\\DevelopmentDashboard"
            }
        }
    }
}
